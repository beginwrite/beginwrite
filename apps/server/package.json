  "volta": {
    "node": "23.7.0",
    "pnpm": "10.2.1"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:prettier": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:prettier --write",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@beginwrite/app-graphql-codegen": "workspace:*",
    "@nestjs/apollo": "^13.0.3",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.10",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/typeorm": "^11.0.0",
    "@prisma/client": "6.4.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "express-session": "^1.18.1",
    "graphql": "^16.10.0",
    "ioredis": "^5.5.0",
    "mysql2": "^3.12.0",
    "node-gyp": "^11.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "ts-morph": "^25.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@eslint/compat": "^1.2.7",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@types/passport": "1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "graphql-upload-minimal": "1.6.1",
    "jest": "29.7.0",
    "lint-staged": "15.4.3",
    "prettier": "3.5.2",
    "prisma": "6.4.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
