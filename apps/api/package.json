{
  "name": "api",
  "private": true,
  "volta": {
    "node": "23.10.0",
    "pnpm": "10.6.5"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --config eslint.config.mjs",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:prettier --write",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "migration:generate": "pnpm drizzle-kit generate",
    "migration:run": "pnpm drizzle-kit migrate",
    "migration:seed": "ts-node --files -r tsconfig-paths/register ./db/seeds/seed.ts",
    "test": "vitest --config ./vitest.config.mts",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --poolOptions.threads.singleThread"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@nestjs/apollo": "^13.0.3",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "drizzle-orm": "^0.41.0",
    "drizzle-seed": "^0.3.1",
    "express-session": "^1.18.1",
    "graphql": "^16.10.0",
    "ioredis": "^5.6.0",
    "mysql2": "^3.14.0",
    "node-gyp": "^11.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "ts-morph": "^25.0.1",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.772.0",
    "@beginwrite/eslint-config": "workspace:*",
    "@beginwrite/graphql-codegen": "workspace:*",
    "@golevelup/ts-vitest": "^0.5.2",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.12",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.11",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/node": "22.13.11",
    "@types/passport": "1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "3.0.9",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.5",
    "eslint": "9.22.0",
    "graphql-upload-minimal": "1.6.1",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "source-map-support": "0.5.21",
    "supertest": "7.1.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.2",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  }
}
